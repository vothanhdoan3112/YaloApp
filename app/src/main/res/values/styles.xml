<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/colorPrimary</item>
    <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppBackTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="Btn_mainblue">
        <item name="android:background">@drawable/button_bg_blue</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="Btn_Gray">
        <item name="android:background">@drawable/button_bg_gray</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="CircularProgress" parent="@style/Theme.AppCompat.Light">
        <item name="colorAccent">@color/spiner_loading_color</item>
    </style>

    <color name="spiner_loading_color">#ff1194ff</color>
</resources>